{
	"training_data": "../data/OpenEA_dataset_v1.1/EN_FR_15K_V1/",
  	"output": "../../output/results/",
  	"dataset_division": "721_5fold/1/",

  	"embedding_module": "IMUSE",
  	"alignment_module": "sharing",
	"search_module": "greedy",
	  
    "dim": 100,
  	"init": "normal",
	"ent_l2_norm": true,
	"rel_l2_norm": true,
	"loss_norm": "L2",
  	"margin": 1.5,
	"loss": "margin_based",

    "neg_sampling": "uniform",
	"neg_triple_num": 2,

	"learning_rate": 0.01,
    "optimizer": "Adam",
	"max_epoch": 1000,
	"batch_size": 5000,

	"batch_threads_num": 2,
	"test_threads_num": 4,

	"ordered": true,
	"start_valid": 10,
	"eval_freq": 10,
	"stop_metric": "hits1",
	"eval_metric": "inner",
	"csls": 10,
	"top_k": [1, 5, 10, 50],
	"no_early": true,
	"is_save": true,
	"eval_norm": true,

	"sim_thresholds_ent": 0.6,
	"sim_thresholds_attr": 0.6,
	"interactive_model_iter_num": 1,
  "is_gpu" : true
}